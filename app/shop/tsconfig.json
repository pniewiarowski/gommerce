{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src",
    "../_shared/hook/index.ts",
    "../_shared/hook/use-backend.ts",
    "../_shared/hook/use-cookies.ts",
    "../_shared/axios",
    "src/atom/center.tsx",
    "src/atom/index.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}